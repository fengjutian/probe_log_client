{
  "name": "probe-log",
  "version": "1.0.10",
  "description": "ProbeLog是一个前端日志系统，它将错误信息记录于本地localStorage中。无任何依赖、无入侵性。",
  "main": "./dist/probe-log.es.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rm -rf ./dist && rollup -c",
    "build:prod": "rollup -c --bundleConfigAsCjs",
    "dev": "serve",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "fengjutian",
  "license": "ISC",
  "keywords": [
    "csijs / felog / tntlog / log / frontendlog"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    ".gitignore",
    ".npmignore"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "eslint": "^7.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "rollup": "^2.47.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "dayjs": "^1.11.10",
    "dexie": "^3.2.4",
    "excellentexport": "^3.7.0"
  }
}
